<UserControl
    x:Class="Constructor5.SituationTemplates.CareerEventSituation.CareerEventSituationTemplateEditor"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:c="clr-namespace:Constructor5.UI.Shared;assembly=Constructor5.UI"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:Constructor5.SituationTemplates.CareerEventSituation"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d">
    <StackPanel>
        <c:Field Label="SituationJob" LabelWidth="100">
            <c:ReferenceControl
                CreateElementFunction="ReferenceControl_CreateElementFunction"
                ElementTypeName="SituationJob"
                Reference="{Binding SituationJob}"
                ShowBrowseButton="True"
                ShowCreateButton="True" />
        </c:Field>
        <c:Field Label="RoleState" LabelWidth="100">
            <c:ReferenceControl
                ElementTypeName="RoleState"
                Reference="{Binding RoleState}"
                ShowBrowseButton="True"
                ShowCreateButton="True" />
        </c:Field>
    </StackPanel>
</UserControl>
