<TuningActions>
  <IfBool p="BlockAging">
    <V n="disable_aging" t="enabled">
      <U n="enabled">
        <IfBool p="BlockAllAging" Inverted="True">
          <L n="allowed_ages">
            <IfBool p="BlockAgingFromBaby" Inverted="True">
              <E>BABY</E>
            </IfBool>
            <IfBool p="BlockAgingFromToddler" Inverted="True">
              <E>TODDLER</E>
            </IfBool>
            <IfBool p="BlockAgingFromChild" Inverted="True">
              <E>CHILD</E>
            </IfBool>
            <IfBool p="BlockAgingFromTeen" Inverted="True">
              <E>TEEN</E>
            </IfBool>
            <IfBool p="BlockAgingFromYoungAdult" Inverted="True">
              <E>YOUNGADULT</E>
            </IfBool>
            <IfBool p="BlockAgingFromAdult" Inverted="True">
              <E>ADULT</E>
            </IfBool>
            <IfBool p="BlockAgingFromElder" Inverted="True">
              <E>ELDER</E>
            </IfBool>
          </L>
        </IfBool>
      </U>
    </V>
  </IfBool>
  
  <IfNotEmpty p="VoiceEffect">
    <V n="voice_effect" t="enabled">
      <U n="enabled">
        <E n="voice_effect">$VoiceEffect</E>
      </U>
    </V>
  </IfNotEmpty>
</TuningActions>