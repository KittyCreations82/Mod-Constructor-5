<Window
    x:Class="Constructor5.UI.Dialogs.CustomTuningDialog.CustomTuningWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:c="clr-namespace:Constructor5.UI.Shared"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:Constructor5.UI.Dialogs.CustomTuningDialog"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="Custom Tuning - The Sims 4 Mod Constructor (V5)"
    Width="600"
    Height="400"
    FontSize="{StaticResource Constructor.BigFontSize}"
    Style="{StaticResource Constructor.WindowStyle}"
    WindowStartupLocation="CenterOwner"
    mc:Ignorable="d">
    <local:CustomTuningDialogControl x:Name="Control" AutoUpdate="False" />
</Window>
