<UserControl
    x:Class="Constructor5.SituationGoalTemplates.Single.SituationGoalTravelTemplateEditor"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:c="clr-namespace:Constructor5.UI.Shared;assembly=Constructor5.UI"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:Constructor5.SituationGoalTemplates.Single"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d">
    <StackPanel>
        <c:Field Label="Venue" LabelWidth="100">
            <StackPanel>
                <c:ReferenceControl ElementTypeName="Venue" Reference="{Binding Venue}" />
                <c:CheckBox Content="SelectOnlyOnOwnedLot" IsChecked="{Binding SelectOnlyOnOwnedLot}" />
            </StackPanel>
        </c:Field>
    </StackPanel>
</UserControl>
