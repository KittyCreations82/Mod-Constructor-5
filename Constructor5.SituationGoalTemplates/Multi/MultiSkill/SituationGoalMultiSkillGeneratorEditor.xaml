<UserControl x:Class="Constructor5.SituationGoalTemplates.Multi.MultiSkill.SituationGoalMultiSkillGeneratorEditor"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:Constructor5.SituationGoalTemplates.Multi.MultiSkill"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:shared="clr-namespace:Constructor5.UI.Shared;assembly=Constructor5.UI" xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit"
             d:DesignHeight="450"
             d:DesignWidth="800"
             mc:Ignorable="d">
    <StackPanel>
        <shared:Field Label="Skill" LabelWidth="100">
            <shared:ReferenceControl Reference="{Binding Skill}" ElementTypeName="Skill" />
        </shared:Field>
        <shared:Field Label="Max Level" LabelWidth="100">
            <xctk:IntegerUpDown Text="{Binding MaxLevel}" />
        </shared:Field>
    </StackPanel>
</UserControl>
