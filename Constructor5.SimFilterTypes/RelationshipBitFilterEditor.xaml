<UserControl x:Class="Constructor5.SimFilterTypes.RelationshipBitFilterEditor"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:Constructor5.SimFilterTypes"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:shared="clr-namespace:Constructor5.UI.Shared;assembly=Constructor5.UI"
             d:DesignHeight="450"
             d:DesignWidth="800"
             mc:Ignorable="d">
    <StackPanel>
        <shared:Field Label="Blacklist" LabelWidth="100">
            <shared:ReferenceListControl ElementReferenceList="{Binding Blacklist}" ElementTypeName="Blacklist" />
        </shared:Field>
        <shared:Field Label="Whitelist" LabelWidth="100">
            <shared:ReferenceListControl ElementReferenceList="{Binding Whitelist}" ElementTypeName="Whitelist" />
        </shared:Field>
    </StackPanel>
</UserControl>
