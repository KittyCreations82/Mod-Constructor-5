<UserControl x:Class="Constructor5.TestConditionTypes.Compound.CompoundConditionEditor"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:Constructor5.TestConditionTypes.Compound"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:shared="clr-namespace:Constructor5.UI.Shared;assembly=Constructor5.UI"
             d:DesignHeight="450"
             d:DesignWidth="800"
             mc:Ignorable="d">
    <d:UserControl.DataContext>
        <local:CompoundCondition />
    </d:UserControl.DataContext>
    <StackPanel>
        <shared:ReferenceControl EditorCategory="Default"
                                 ElementTypeName="CompoundConditionElement"
                                 Reference="{Binding CompoundConditionElement}"
                                 ShowCreateButton="True" />
    </StackPanel>
</UserControl>
