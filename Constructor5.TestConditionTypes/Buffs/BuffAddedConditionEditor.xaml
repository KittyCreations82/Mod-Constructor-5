<UserControl x:Class="Constructor5.TestConditionTypes.Buffs.BuffAddedConditionEditor"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Constructor5.TestConditionTypes.Buffs" xmlns:shared="clr-namespace:Constructor5.UI.Shared;assembly=Constructor5.UI"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <StackPanel>
        <shared:Field Label="Buffs" LabelWidth="100">
            <shared:ReferenceListControl ElementReferenceList="{Binding Buffs}" ElementTypeName="Buff" />
        </shared:Field>
    </StackPanel>
</UserControl>
