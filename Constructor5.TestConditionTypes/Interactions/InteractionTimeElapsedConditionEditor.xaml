<UserControl x:Class="Constructor5.TestConditionTypes.Interactions.InteractionTimeElapsedConditionEditor"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:Constructor5.TestConditionTypes.Interactions"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:shared="clr-namespace:Constructor5.UI.Shared;assembly=Constructor5.UI"
             xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit"
             d:DesignHeight="450"
             d:DesignWidth="800"
             mc:Ignorable="d">
    <StackPanel>
        <shared:Field Label="Sim Minutes" LabelWidth="100">
            <xctk:IntegerUpDown Text="{Binding SimMinutes}" />
        </shared:Field>
        <shared:Field Label="Tag" LabelWidth="100">
            <shared:TextBoxWithPresetsControl PresetsFolder="InteractionTag" Text="{Binding Tag}" />
        </shared:Field>
    </StackPanel>
</UserControl>
