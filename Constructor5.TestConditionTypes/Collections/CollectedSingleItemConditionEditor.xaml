<UserControl x:Class="Constructor5.TestConditionTypes.Collections.CollectedSingleItemConditionEditor"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:Constructor5.TestConditionTypes.Collections"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:c="clr-namespace:Constructor5.UI.Shared;assembly=Constructor5.UI"
             d:DesignHeight="450"
             d:DesignWidth="800"
             mc:Ignorable="d">
    <StackPanel>
        <c:Field Label="Collection Types" LabelWidth="150">
            <c:StringListControl PresetsFolder="Collection" StringList="{Binding CollectionTypes}" />
        </c:Field>
    </StackPanel>
</UserControl>
