<UserControl x:Class="Constructor5.Elements.Commodities.CommodityStatesComponentEditor"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:c="clr-namespace:Constructor5.UI.Shared;assembly=Constructor5.UI"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:Constructor5.Elements.Commodities"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             d:DesignHeight="450"
             d:DesignWidth="800"
             mc:Ignorable="d">
    <ScrollViewer>
        <c:LeftRightMargin>
            <StackPanel>
                <c:SelectableObjectControl EditorCategory="Invalid"
                                           Object="{Binding Generator}"
                                           TypeRegistryCategory="CommodityStateGenerators" />
                <c:ObjectEditorControl EditorCategory="Default" Object="{Binding Generator}" />
            </StackPanel>
        </c:LeftRightMargin>
    </ScrollViewer>
</UserControl>
