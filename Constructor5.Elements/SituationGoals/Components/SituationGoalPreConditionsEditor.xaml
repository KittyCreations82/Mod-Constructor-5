<UserControl x:Class="Constructor5.Elements.SituationGoals.Components.SituationGoalPreConditionsEditor"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:conditions="clr-namespace:Constructor5.Elements.TestConditions"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:Constructor5.Elements.SituationGoals.Components"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:shared="clr-namespace:Constructor5.UI.Shared;assembly=Constructor5.UI"
             d:DesignHeight="450"
             d:DesignWidth="800"
             mc:Ignorable="d">
    <d:DataContext>
        <local:SituationGoalPreConditionsComponent />
    </d:DataContext>
    <ScrollViewer Margin="{StaticResource Constructor.BigPaddingMinusTop}">
        <conditions:ConditionListControl ConditionList="{Binding Conditions}" />
    </ScrollViewer>
</UserControl>
