<UserControl x:Class="Constructor5.Elements.Traits.Components.TraitOriginControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Constructor5.Elements.Traits.Components" xmlns:c="clr-namespace:Constructor5.UI.Shared;assembly=Constructor5.UI"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <c:Field Label="TraitOrigin" LabelWidth="100">
        <StackPanel>
            <c:STBLStringControl Text="{Binding TraitOrigin}" />
            <c:TextBlock Text="TraitOriginNotice" />
        </StackPanel>
    </c:Field>
</UserControl>
