<UserControl x:Class="Constructor5.Elements.Interactions.Career.CareerInteractionEditor"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:c="clr-namespace:Constructor5.UI.Shared;assembly=Constructor5.UI"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:Constructor5.Elements.Interactions.Career"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             d:DesignHeight="450"
             d:DesignWidth="800"
             mc:Ignorable="d">
    <c:TabControl>
        <c:TabItem Header="Balloons">
            <c:SpacedStack TopMargin="True">
                <c:ReferenceControlWithEditorUnderneath CreateElementFunction="CreateBalloonSetFunction"
                                                        EditorTag="NoSpeech"
                                                        ElementTypeName="BalloonSet"
                                                        Reference="{Binding BalloonSet}" />
            </c:SpacedStack>
        </c:TabItem>
    </c:TabControl>
</UserControl>
