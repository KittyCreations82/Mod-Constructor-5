<UserControl x:Class="Constructor5.Elements.Interactions.Shared.InteractionLockOutEditor"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:Constructor5.Elements.Interactions.Shared"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:c="clr-namespace:Constructor5.UI.Shared;assembly=Constructor5.UI"
             d:DesignHeight="450"
             d:DesignWidth="800"
             mc:Ignorable="d">
    <StackPanel>
        <StackPanel Margin="{StaticResource Constructor.BigPaddingMinusTop}">
            <c:TextBlock Text="LockOutTimeInitialDetailed" />
            <c:IntBoundsControl Bounds="{Binding InitialLockOutTime}" />
        </StackPanel>
        <StackPanel Margin="{StaticResource Constructor.BigPaddingMinusTop}">
            <c:TextBlock Text="LockOutTimeDetailed" />
            <c:IntBoundsControl Bounds="{Binding LockOutTime}" />
        </StackPanel>
    </StackPanel>
</UserControl>
