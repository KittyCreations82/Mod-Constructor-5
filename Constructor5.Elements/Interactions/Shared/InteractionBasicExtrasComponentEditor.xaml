<UserControl
    x:Class="Constructor5.Elements.Interactions.Shared.InteractionBasicExtrasComponentEditor"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:c="clr-namespace:Constructor5.UI.Shared;assembly=Constructor5.UI"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:interactionbasicextras="clr-namespace:Constructor5.Elements.InteractionBasicExtras"
    xmlns:local="clr-namespace:Constructor5.Elements.Interactions.Shared"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d">
    <ScrollViewer>
        <c:LeftRightMargin>
            <StackPanel>
                <c:TextBlock Text="InteractionBasicExtrasNotice" />
                <interactionbasicextras:BasicExtrasControl DataContext="{Binding BasicExtras}" />
            </StackPanel>
        </c:LeftRightMargin>
    </ScrollViewer>
</UserControl>
