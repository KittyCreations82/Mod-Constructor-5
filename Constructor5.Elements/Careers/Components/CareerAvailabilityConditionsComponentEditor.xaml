<UserControl x:Class="Constructor5.Elements.Careers.Components.CareerAvailabilityConditionsComponentEditor"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:c="clr-namespace:Constructor5.UI.Shared;assembly=Constructor5.UI"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:Constructor5.Elements.Careers.Components"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:testconditions="clr-namespace:Constructor5.Elements.TestConditions"
             d:DesignHeight="450"
             d:DesignWidth="800"
             mc:Ignorable="d">
    <c:LeftRightMargin>
        <c:SpacedStack>
            <c:TextBlock Text="CareerAvailabilityConditionsNotice" />
            <testconditions:ConditionListControl ConditionList="{Binding Conditions}" />
        </c:SpacedStack>
    </c:LeftRightMargin>
</UserControl>
