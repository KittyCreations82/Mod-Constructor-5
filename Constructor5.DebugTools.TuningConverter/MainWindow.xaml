<Window x:Class="Constructor5.DebugTools.TuningConverter.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        Title="Tuning Converter"
        Width="800"
        Height="450"
        WindowStyle="ToolWindow"
        mc:Ignorable="d">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*" />
            <RowDefinition Height="Auto" />
            <RowDefinition Height="Auto" />
        </Grid.RowDefinitions>
        <TextBox x:Name="TextBox" VerticalScrollBarVisibility="Visible" />
        <Button x:Name="ConvertButton"
                Grid.Row="2"
                Click="ConvertButton_OnClick"
                Content="Convert from Clipboard" />
    </Grid>
</Window>
